<!DOCTYPE html>
<html>
<head>
  <title>TrackExpenses</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>

	<div class="main-contain">
		<div class="visible-xs visible-nine-nine-two">
			<%= render 'partials/navbar' %>
		</div>
		<div class="container">
			<div class="row">
				<div class="col-lg-2 col-md-3 hidden-xs hidden-sm sidebar">
					<%= render 'partials/sidebar_navbar' %>
				</div>
				<div class="col-lg-10 col-lg-offset-2 col-md-9 col-md-offset-3">
					<% unless notice.nil? %>
						<p class="notice"><%= notice %></p>
						<p class="alert"><%= alert %></p>
					<% end %>
					<%= yield %>
				</div>
			
			</div>
		</div>
	</div>
</body>
</html>
